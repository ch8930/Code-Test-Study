    "# 실전 문제3 - 개미전사\n",
    "\n",
    "n = int(input())\n",
    "stock = list(map(int, input().split()))\n",
    "print(stock)\n",
    "\n",
    "d = [0]*101\n",
    "\n",
    "d[1] = stock[0]\n",
    "d[2] = max(stock[0],stock[1])\n",
    "\n",
    "for i in range(3, n+1):\n",
    "    d[i] = max(stock[i-1]+d[i-2], d[i-1])\n",
    "\n",
    "print(d[n])   "
